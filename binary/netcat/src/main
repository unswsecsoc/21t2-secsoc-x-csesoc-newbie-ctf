#!/usr/bin/env python3
import sys

print("Meowdy! Congratulations on successfully connecting!")
sys.stdout.flush()

resp = input(
    'To get the flag, type a "meow" back at me, and press the enter key so I can hear it! '
)
sys.stdout.flush()

while resp.lower().strip() != "meow":
    resp = input("That doesn't sound like a meow. Try again :) ")
    sys.stdout.flush()
print()
print(r"Hey ma! I'm a cool cybercat with a flag now! NEWBIE{y33t_i_n3tme0WeD}")
sys.stdout.flush()
